<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- CardSlideView -->
    <declare-styleable name="card">
        <attr name="bottomMarginTop" format="dimension" />
        <attr name="yOffsetStep" format="dimension" />
    </declare-styleable>

    <!-- 图书馆滚动Imageview -->
    <declare-styleable name="BezelImageView">
        <attr name="maskDrawable" format="reference" />
        <attr name="borderDrawable" format="reference" />
        <attr name="desaturateOnPress" format="boolean" />
    </declare-styleable>

    <!-- 书库Menu -->
    <declare-styleable name="CoolMenuFrameLayout">
        <attr name="num" format="enum">
            <enum name="one" value="1" />
            <enum name="two" value="2" />
            <enum name="three" value="3" />
            <enum name="four" value="4" />
            <enum name="five" value="5" />
        </attr>
        <attr name="titleSize" format="dimension"/>
        <attr name="titleColor" format="color"/>
        <attr name="titleIcon" format="reference"/>
    </declare-styleable>

</resources>